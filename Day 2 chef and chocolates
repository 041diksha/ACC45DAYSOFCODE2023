#include<iostream>
using namespace std;
int main(){
    int x , y , z , T , i=0;
    cout<<"how many test you want : ";
    cin>>T;
    if(T<=100&&T>=1){
        while(i<T){ 
        cout<<"enter the no. of 5rs of coin :"<<endl;
        cin>>x;
        cout<<"enter the no. of 10rs of coin :"<<endl;
        cin>>y;
        cout<<"enter the amount of chocolate :"<<endl;
        cin>>z;
        int a;
        a = (x*5+y*10)/z;
        cout<<"the maximum no. of chocolate :"<<a<<endl;
        i++;
    }
    }
    return 0;
}
